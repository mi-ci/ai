from bs4 import BeautifulSoup
import requests

a=[]

base_url = "https://lolchess.gg/leaderboards?region=kr&mode=ranked"

# keyword = input("검색어를 입력하세요 : ")

search_url = base_url 

r = requests.get(search_url)

soup = BeautifulSoup(r.text, "html.parser")

items = soup.select("div[class=news_tit]")

for i in soup.select("span"):
    a.append(i.text)
print(a)
